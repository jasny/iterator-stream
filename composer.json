{
    "name": "jasny/iterator-stream",
    "description": "Output streams for iterators, formatting and writing one element at a time",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/iterator-stream/issues",
        "source": "https://github.com/jasny/iterator-stream"
    },
    "require": {
        "php": ">=7.1.0",
        "jasny/php-functions": "^4.1"
    },
    "require-dev": {
        "ext-gd": "*",
        "jasny/php-code-quality": "^2.3",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\IteratorStream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\IteratorStream\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
